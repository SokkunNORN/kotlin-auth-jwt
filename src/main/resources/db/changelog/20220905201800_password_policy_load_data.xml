<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="password-policy-load-data" author="sokkun">
        <loadData encoding="UTF-8"
                  file="db/seed/password-policy.csv"
                  separator=","
                  tableName="password_policy"
                  primaryKey="id">
            <column name="id" type="NUMERIC"/>
            <column name="reset_in" type="NUMERIC"/>
            <column name="notify_before" type="NUMERIC"/>
            <column name="min_num_upper_letter" type="NUMERIC"/>
            <column name="min_num_lower_letter" type="NUMERIC"/>
            <column name="min_num_numeric" type="NUMERIC"/>
            <column name="min_num_special_char" type="NUMERIC"/>
            <column name="min_length" type="NUMERIC"/>
            <column name="created_at" type="TIMESTAMP"/>
            <column name="status_id" type="NUMERIC"/>
        </loadData>
    </changeSet>
</databaseChangeLog>